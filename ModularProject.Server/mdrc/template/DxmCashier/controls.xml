<controls>
    <group name="list">
        <vue>
            <list for="(row,index) in list" item="row" key="row.ID"/>
        </vue>
        <row onmouseout="this.style.backgroundColor=&apos;&apos;" onmouseover="this.style.backgroundColor=&apos;#f0f0f0&apos;"/>
        <cell/>
        <check tag-name="div" class="x-check"/>
        <table/>
    </group>
    <group name="form">
        <line tag-name="div" class="x-line"/>
        <title tag-name="div" class="x-title"/>
        <content tag-name="div" class="x-content"/>
        <liner tag-name="div" class="x-liner"/>
        <day-picker tag-name="div" class="x-day-picker"/>
        <picker tag-name="div" class="x-picker"/>
        <check tag-name="div" class="x-check"/>
    </group>
</controls>